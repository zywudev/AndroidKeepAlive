<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wuzy.aidlclient">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".LocalService"
            android:exported="true"
            android:enabled="true"/>

        <activity android:name=".TransferActivity"
            android:theme="@style/TransparentStyle">
            <intent-filter>
                <action android:name="com.wuzy.aidlclient.TransferActivity.FROM_SELF"/>
                <action android:name="com.wuzy.aidlclient.TransferActivity.FROM_OTHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>